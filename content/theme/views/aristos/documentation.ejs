<% include ../_layouts/aristosHeader %>
    <h1 class="page__title">Documentation</h1>
    <div class="wrapper">
        <div class="documentation__categories">
            <h2>Docs</h2>
            <% documentation.forEach((docs)=>{ %>
                <a href="/aristos/documentation/<%= docs._id %>"><%= docs.title %></a>
            <% }) %>
        </div>
        <div class="documentation__content">
            <% documentationItem.forEach((doc)=>{ %>
                <h2 class="documentation__content__title"><%= doc.title %></h2>
                <%- doc.content %>
            <% }) %>
        </div>
    </div>
<% include ../_layouts/footer %>